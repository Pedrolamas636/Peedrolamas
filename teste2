local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "PORN HUB", HidePremium = false, SaveConfig = false, ConfigFolder = "PORNHUB1"})

local Tab = Window:MakeTab({
	Name = "Scripts",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
 
Tab:AddButton({
	Name = "Yuto Hub",
	Callback = function()
	setclipboard("V2LEvuL7Bx3Ah!ES2")
	toclipboard("V2LEvuL7Bx3Ah!ES2")
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Binintrozza/yutv2e/main/afss"))()
  	end    
})

